{
    "name": "modera/backend-on-steroids-bundle",
    "type": "symfony-bundle",
    "description": "Bundle makes it easier to optimize MJR based backend's loading speed for up to 50%.",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.0",
        "sensio/generator-bundle": "~2.3|~3.0",
        "sergeil/expander-bundle": "~1.0",
        "modera/mjr-integration-bundle": "~2.0|~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phake/phake": "2.*",
        "modera/foundation-bundle": "~1.0|~2.0"
    },
    "autoload": {
        "psr-0": { "Modera\\BackendOnSteroidsBundle": "" }
    },
    "target-dir": "Modera/BackendOnSteroidsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
